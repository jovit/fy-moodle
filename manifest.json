{
  "manifest_version": 2,

  "name": "*FY* MOODLE",
  "version": "1.0.0",
  "description": "Nothing to say",
  "default_locale": "en",
  "icons": { "16": "icon/rework-16.png",
           "48": "icon/rework-48.png",
          "128": "icon/rework-128.png" },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["webNavigation", "storage"],
  "content_scripts": [
    {
      "matches": ["http://www.ggte.unicamp.br/eam/mod/quiz/review.php*"],
      "js": ["firebase.js", "scan.js"],
      "run_at": "document_end",
      "all_frames": true
    },
    {
      "matches": ["http://www.ggte.unicamp.br/eam/mod/quiz/attempt.php*"],
      "js": ["firebase.js", "answer.js"],
      "run_at": "document_end",
      "all_frames": true
    }
  ]
}
